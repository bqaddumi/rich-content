{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://wix-rich-content/unsplash-content-data-schema.json",
  "type": "object",
  "properties": {
    "image": {
      "$id": "/properties/image",
      "type": "object",
      "properties": {
        "alt": {
          "$id": "/properties/image/properties/alt",
          "type": "string",
          "title": "Alt",
          "default": "",
          "examples": ["Unsplash"]
        },
        "caption": {
          "$id": "/properties/image/properties/caption",
          "type": "string",
          "title": "Caption",
          "default": "",
          "examples": ["Unsplash"]
        },
        "height": {
          "$id": "/properties/image/properties/height",
          "type": "number",
          "title": "Photo Height",
          "default": 100
        },
        "width": {
          "$id": "/properties/image/properties/width",
          "type": "number",
          "title": "Photo Width",
          "default": 100
        },
        "isValidUrl": {
          "$id": "/properties/image/properties/isValidUrl",
          "type": "boolean",
          "title": "URL Validation",
          "default": true,
          "examples": [true, false]

        },
        "nofollow": {
          "$id": "/properties/image/properties/nofollow",
          "type": "boolean",
          "title": "Rel value",
          "default": true,
          "examples": [true, false]
        },
        "originalUrl": {
          "$id": "/properties/image/properties/originalUrl",
          "type": "string",
          "title": "Photo URL",
          "default": "https://images.unsplash.com/photo-1558981403-c5f9899a28bc?ixlib=rb-1.2.1&q=85&fm=jpg&crop=entropy&cs=srgb&ixid=eyJhcHBfaWQiOjY4OTI4fQ"
        },
        "targetBlank": {
          "$id": "/properties/image/properties/targetBlank",
          "type": "boolean",
          "title": "Target Value",
          "default": true,
          "examples": [true, false]
        },
        "url": {
          "$id": "/properties/image/properties/url",
          "type": "string",
          "title": "url",
          "default": "https://example-link.com/",
          "examples": ["google.com"]
        },
        "username": {
          "$id": "/properties/image/properties/username",
          "type": "string",
          "title": "username",
          "default": ""
        }
      }
    },
    "config": {
      "$id": "/properties/config",
      "type": "object",
      "properties": {
        "size": {
          "$id": "/properties/config/properties/size",
          "type": "string",
          "title": "size",
          "default": "center"
        },
        "alignment": {
          "$id": "/properties/config/properties/alignment",
          "type": "string",
          "title": "alignment",
          "default": "center",
          "examples": ["center", "right", "left"]
        }
      }
    }
  }
}
