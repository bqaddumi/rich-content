{
  "$id": "http://path/to/schema.json",
  "type": "object",
  "properties": {
    "youtube": {
      "$id": "/properties/youtube",
      "type": "object",
      "properties": {
        "url": {
          "$id": "/properties/youtube/properties/url",
          "type": "string",
          "title": "URL",
          "default": "",
          "examples": ["https://www.youtube.com/watch?v=JfVbgwmo2UA "]
        },
        "config": {
          "$id": "/properties/config",
          "type": "object",
          "properties": {
            "size": {
              "$id": "/properties/config/properties/size",
              "type": "string",
              "title": "size",
              "default": "content"
            },
            "alignment": {
              "$id": "/properties/config/properties/alignment",
              "type": "string",
              "title": "alignment",
              "default": "center",
              "examples": ["center", "right", "left"]
            },
            "key": {
              "$id": "/properties/config/properties/key",
              "type": "string",
              "title": "The Key Schema ",
              "default": "",
              "examples": ["78m20"]
            }
          }
        }
      }
    },
    "required": ["url"]
  }
}
